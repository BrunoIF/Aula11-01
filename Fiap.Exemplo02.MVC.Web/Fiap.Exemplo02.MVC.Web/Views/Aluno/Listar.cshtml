@model Fiap.Exemplo02.MVC.Web.Models.Aluno

@{
    ViewBag.Title = "Listar";
}

<h2>Listar</h2>



    <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nome</th>
                    <th>Data Nascimento</th>
                    <th>Bolsa</th>
                    <th>Desconto</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in lista)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Nome</td>
                        <td>@item.DataNascimento</td>
                        <td>@item.Bolsa</td>
                        <td>@item.Desconto</td>

                    </tr>
                }
            </tbody>

        </table>
